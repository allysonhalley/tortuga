<p id="notice"><%= notice %></p>

<h1>Locales</h1>

<table>
  <thead>
    <tr>
      <th>Street</th>
      <th>Number</th>
      <th>Cep</th>
      <th>Complement</th>
      <th>Lat</th>
      <th>Long</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @locales.each do |locale| %>
      <tr>
        <td><%= locale.street %></td>
        <td><%= locale.number %></td>
        <td><%= locale.cep %></td>
        <td><%= locale.complement %></td>
        <td><%= locale.lat %></td>
        <td><%= locale.long %></td>
        <td><%= link_to 'Show', locale %></td>
        <td><%= link_to 'Edit', edit_locale_path(locale) %></td>
        <td><%= link_to 'Destroy', locale, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Locale', new_locale_path %>
